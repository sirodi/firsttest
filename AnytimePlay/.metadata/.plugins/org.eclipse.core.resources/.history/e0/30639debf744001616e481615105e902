package com.atp.core.services.rest;

import java.util.List;

/**
 * @author sirodi
 *
 */
public class CourtDetailDTO {
	private String courtId;
	
	private String courtName;
	public String getCourtName() {
		return courtName;
	}
	public void setCourtName(String courtName) {
		this.courtName = courtName;
	}
	public List<CourtSlotsDTO> getCourtAllotmentList() {
		return courtAllotmentList;
	}
	public void setCourtAllotmentList(List<CourtSlotsDTO> courtAllotmentList) {
		this.courtAllotmentList = courtAllotmentList;
	}
	private List<CourtSlotsDTO> courtAllotmentList;
	

}
